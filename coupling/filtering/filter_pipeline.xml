<?xml version ="1.0"?>

<!-- XML config file used by MaMiCo's FilterPipeline -->

<filter-pipeline>

    <per-instance output="sequence-1">
        <sequence-1 input = "md">
            <write-to-file 
                file = "testwtf"
                type = "csv"
            />
        </sequence-1>

        <sequence-2 input="sequence-1">
            <filter_y 
            />
            <filter_z 
                param1 = "..."
            /> 
            <write-to 
                file = "file_yz"
            />
        </sequence-2>

        <sequence-3 input="sequence-1">
            <filter_z param5 = "..."/> 
            <filter_y param6 = "..."/> 
            <write-to file = "file_zy"/>
        </sequence-3> 

    </per-instance>
    

    <post-multi-instance output="multimd">
        <!-- Beispiel einer leeren Sequenz mit den von MultiMDCellService gemittelten Cells ("multimd") als Input -->
        <sequence-1 input = "multimd"/>

        <!-- Falls post-multi-instance-filtering nicht gebraucht ist, einfach output = input -->
    </post-multi-instance>

</filter-pipeline>
