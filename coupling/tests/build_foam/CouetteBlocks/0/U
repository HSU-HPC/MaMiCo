FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            movingWallVelocity;
        value           uniform (1.5 0 0);
    }

    fixedWall
    {
        type            noSlip;
    }

    front
    {
        type            zeroGradient;
    }
    
    back
    {
        type            zeroGradient;
    }
    
    left
    {
        type            zeroGradient;
    }
    
    right
    {
        type            zeroGradient;
    }
    
    fronti
    {
        type 		zeroGradient;
    }
    
    backi
    {
        type 		zeroGradient;
    }
    
    lefti
    {
        type 		zeroGradient;
    }
    
    righti
    {
        type 		zeroGradient;
            }
    
    topi
    {
        type 		zeroGradient;
            }
    
    buttomi
    {
        type 		zeroGradient;    }
}

