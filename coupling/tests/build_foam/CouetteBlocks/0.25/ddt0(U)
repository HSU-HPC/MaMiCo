FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.25";
    object      ddt0(U);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    fixedWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    front
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    back
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    left
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    right
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    fronti
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    backi
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    lefti
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    righti
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    topi
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    buttomi
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


