<?xml version ="1.0"?>

<!-- XML config file used by MaMiCo's FilterPipeline -->

<filter-pipeline>

    <per-instance output="sequence-1">
		<sequence-1 input="md" domain-start="1;1;1" domain-end="3 ; 3 ; 3">
            <write-to-file 
                location="testwtf.csv"
			/>
			<write-to-file 
                location="testwtf2.csv"
            />
			<gauss
			/>
        </sequence-1>

        <sequence-2 input="sequence-1" domain-start="5;5;5" domain-end="10;10;10">
			<write-to-file 
                location="file_yz"
            />
			<gauss
			/>
        </sequence-2>

        <sequence-3 input="sequence-1">
			<write-to-file 
				location="file_zy"
				/>
        </sequence-3> 

    </per-instance>
    

    <post-multi-instance output="md">
        <!-- Beispiel einer leeren Sequenz mit den von MultiMDCellService gemittelten Cells ("multimd") als Input -->
        <sequence-1 input="md"/>

        <!-- Falls post-multi-instance-filtering nicht gebraucht ist, einfach output = input -->
    </post-multi-instance>

</filter-pipeline>
