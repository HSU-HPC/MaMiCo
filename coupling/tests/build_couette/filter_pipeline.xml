<?xml version ="1.0"?>

<!-- XML config file used by MaMiCo's FilterPipeline -->

<filter-pipeline>

    <per-instance output="sequence-1">
        <sequence-1 input="md">
            <write-to-file 
				domain-start="0 ; 0 ; 0"
				domain-end="2 ; 3 ; 4"
                location="testwtf.csv"
            />
        </sequence-1>

        <sequence-2 input="sequence-1">
			<write-to-file 
				domain-start="0 ; 0 ; 0"
				domain-end="0 ; 0 ; 0"
                location="file_yz"
            />
        </sequence-2>

        <sequence-3 input="sequence-1">
			<write-to-file 
				domain-start="0 ; 0 ; 0"
				domain-end="10 ; 10 ; 10"
				location="file_zy"
				/>
        </sequence-3> 

    </per-instance>
    

    <post-multi-instance output="md">
        <!-- Beispiel einer leeren Sequenz mit den von MultiMDCellService gemittelten Cells ("multimd") als Input -->
        <sequence-1 input="md"/>

        <!-- Falls post-multi-instance-filtering nicht gebraucht ist, einfach output = input -->
    </post-multi-instance>

</filter-pipeline>
